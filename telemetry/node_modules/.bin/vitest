#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gavinleroy/Dev/Misc/oxcaml-tests/activities/telemetry/node_modules/.pnpm/vitest@3.2.4_@types+node@24.7.0_jsdom@27.0.0_postcss@8.5.6__yaml@2.8.1/node_modules/vitest/node_modules:/Users/gavinleroy/Dev/Misc/oxcaml-tests/activities/telemetry/node_modules/.pnpm/vitest@3.2.4_@types+node@24.7.0_jsdom@27.0.0_postcss@8.5.6__yaml@2.8.1/node_modules:/Users/gavinleroy/Dev/Misc/oxcaml-tests/activities/telemetry/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gavinleroy/Dev/Misc/oxcaml-tests/activities/telemetry/node_modules/.pnpm/vitest@3.2.4_@types+node@24.7.0_jsdom@27.0.0_postcss@8.5.6__yaml@2.8.1/node_modules/vitest/node_modules:/Users/gavinleroy/Dev/Misc/oxcaml-tests/activities/telemetry/node_modules/.pnpm/vitest@3.2.4_@types+node@24.7.0_jsdom@27.0.0_postcss@8.5.6__yaml@2.8.1/node_modules:/Users/gavinleroy/Dev/Misc/oxcaml-tests/activities/telemetry/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
